
#include <bits/stdc++.h>
using namespace std;

#define int long long

void solve() {

    int n;
    cin>>n;

    cout<<1<<" ";

    for(int i=2;i<n;i++){
        cout<<i+1<<" ";
    }

    cout<<2<<endl;

}

int32_t main() {

    int t;
    cin >> t;

    while (t--) {

        solve();

    }

    return 0;
}
